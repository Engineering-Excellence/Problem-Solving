   "source": [
    "# 조건에 맞게 수열 변환하기 1\n",
    "def solution(arr):\n",
    "    for i, n in enumerate(arr):\n",
    "        if n >= 50 and n % 2 == 0:\n",
    "            arr[i] //= 2\n",
    "        elif n < 50 and n % 2 == 1:\n",
    "            arr[i] *= 2\n",
    "    return arr\n",
    "\n",
    "\n",
    "print(solution([1, 2, 3, 100, 99, 98]))"
   ],
